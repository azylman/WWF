<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WWFSolver</servlet-name>
		<servlet-class>wwf_solver.WWFSolverServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TestWord</servlet-name>
		<servlet-class>wwf_solver.TestWordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WWFSolver</servlet-name>
		<url-pattern>/solve</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TestWord</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>solver.jsp</welcome-file>
	</welcome-file-list>
</web-app>
